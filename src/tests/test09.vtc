varnishtest "Test array prepend 2"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
	import json from "${vmod_topbuild}/src/.libs/libvmod_json.so";

	sub vcl_deliver {
		json.integer(".foo[>]", 1337);
		json.integer(".foo[>]", 1338);
		set resp.http.json = json.dump("");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.json == "{\"foo\": [1338, 1337]}"
}

client c1 -run
