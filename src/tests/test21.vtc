varnishtest "Test object insertion"

server s1 {
	rxreq
	txresp
} -start

varnish v1 -vcl+backend {
	import json from "${vmod_topbuild}/src/.libs/libvmod_json.so";

	sub vcl_deliver {
		json.object(".foo");
		set resp.http.json = json.dump("");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.json == "{\"foo\": {}}"
}

client c1 -run
